<html>
    <head>
        <title><%= title %></title>
    </head>
    <body>
        <ul>
            <a href="/new">Add a new message!</a> <br>
            <% messages.forEach((message) => { %>
                <%= message.user %>:
                <%= message.text %> <br>
                <%= message.added %> <br>
            <% }); %>                
        </ul>
    </body>
</html>